{
  "name": "kirby-boilerplate",
  "version": "1.6.7",
  "license": "MIT",
  "homepage": "https://github.com/FabienGreard/kirby-boilerplate#readme",
  "author": "Fabien Gr√©ard <grd.fabien@gmail.com> (http://fabiengreard.com/)",
  "description": "This is an opiniated boilerplate with rocking tools for front-end development",
  "keywords": [
    "boilerplate",
    "typescript",
    "react",
    "react-query",
    "graphql-request",
    "react-intl",
    "jest",
    "storybook",
    "testing-library",
    "tailwinds",
    "cypress",
    "apollo",
    "graphql",
    "nextjs",
    "prettier",
    "eslint",
    "husky",
    "commitlint",
    "standard-version",
    "github-actions"
  ],
  "scripts": {
    "bootstrap": "node scripts/bootstrap.js",
    "dev": "next dev",
    "dev:test": "jest --watch",
    "dev:coverage": "jest --coverage --watch",
    "dev:e2e": "cypress open",
    "build": "ANALYZE=true next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "tsc": "tsc --noEmit",
    "test": "jest --silent",
    "coverage": "jest --coverage --silent",
    "e2e": "cypress run --headless",
    "storybook": "start-storybook -p 6006",
    "bump:patch": "standard-version --release-as patch --no-verify",
    "bump:minor": "standard-version --release-as minor --no-verify",
    "bump:major": "standard-version --release-as major --no-verify",
    "codegen": "graphql-codegen -r dotenv/config",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "clear": "node scripts/clear.js",
    "prepare": "husky install && prisma generate"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.4",
    "@prisma/client": "^3.9.1",
    "apollo-server-micro": "^3.4.0",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^12.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.21.0",
    "react-query": "^3.32.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/typescript": "^2.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-react-query": "^3.1.1",
    "@next/eslint-plugin-next": "^12.0.1",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.33",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "cypress": "^8.7.0",
    "dotenv": "^10.0.0",
    "eslint": "<8.0.0",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prisma": "^3.4.2",
    "standard-version": "^9.3.2",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
