{
  "name": "kirby-boilerplate",
  "version": "1.4.2",
  "license": "MIT",
  "homepage": "https://github.com/FabienGreard/kirby-boilerplate#readme",
  "author": "Fabien Gr√©ard <grd.fabien@gmail.com> (http://fabiengreard.com/)",
  "description": "This is an opiniated boilerplate with rocking tools for front-end development",
  "keywords": [
    "boilerplate",
    "typescript",
    "react",
    "react-intl",
    "jest",
    "storybook",
    "testing-library",
    "tailwinds",
    "cypress",
    "apollo",
    "graphql",
    "nextjs",
    "prettier",
    "eslint",
    "husky",
    "commitlint",
    "standard-version",
    "github-actions"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:test": "jest --watch",
    "dev:coverage": "jest --coverage --watch",
    "dev:e2e": "cypress open",
    "build": "ANALYZE=true next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "test": "jest --silent",
    "coverage": "jest --coverage --silent",
    "e2e": "cypress run --headless",
    "storybook": "start-storybook -p 6006",
    "bump:patch": "standard-version --release-as patch --no-verify",
    "bump:minor": "standard-version --release-as minor --no-verify",
    "bump:major": "standard-version --release-as major --no-verify",
    "apollo:download": "npx apollo service:download --endpoint=http://localhost:3000/api/graphql graphql-schema.json",
    "apollo:generate": "npx apollo codegen:generate types --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql --outputFlat --tsFileExtension=d.ts",
    "apollo:ts": "node apollo-types.js",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:generate": "prisma generate",
    "clear": "node scripts/clear.js",
    "prepare": "husky install && prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@next/bundle-analyzer": "^10.2.3",
    "@prisma/client": "^2.29.1",
    "apollo-server-micro": "^2.25.0",
    "graphql": "^15.5.0",
    "next": "^10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "cypress": "^7.4.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.0.3",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "prisma": "^2.23.0",
    "standard-version": "^9.3.0",
    "tailwindcss": "^2.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
