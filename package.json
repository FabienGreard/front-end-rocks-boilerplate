{
  "name": "kirby-boilerplate",
  "version": "1.3.1",
  "license": "MIT",
  "homepage": "https://github.com/FabienGreard/kirby-boilerplate#readme",
  "author": "Fabien Gr√©ard <grd.fabien@gmail.com> (http://fabiengreard.com/)",
  "description": "This is an opiniated boilerplate with rocking tools for front-end development",
  "keywords": [
    "boilerplate",
    "typescript",
    "react",
    "react-intl",
    "jest",
    "storybook",
    "testing-library",
    "tailwinds",
    "cypress",
    "apollo",
    "graphql",
    "nextjs",
    "prettier",
    "eslint",
    "husky",
    "commitlint",
    "standard-version",
    "github-actions"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:test": "jest --watch",
    "dev:coverage": "jest --coverage --watch",
    "dev:e2e": "cypress open",
    "build": "ANALYZE=true next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "test": "jest --silent",
    "coverage": "jest --coverage --silent",
    "e2e": "cypress run --headless",
    "storybook": "start-storybook -p 6006",
    "bump:patch": "standard-version --release-as patch --no-verify",
    "bump:minor": "standard-version --release-as minor --no-verify",
    "bump:major": "standard-version --release-as major --no-verify",
    "apollo:download": "npx apollo service:download --endpoint=http://localhost:3000/api/graphql graphql-schema.json",
    "apollo:generate": "npx apollo codegen:generate types --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql --outputFlat --tsFileExtension=d.ts",
    "apollo:ts": "node apollo-types.js",
    "clear": "node scripts/clear.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@next/bundle-analyzer": "^10.2.0",
    "apollo-server-micro": "^2.24.0",
    "graphql": "^15.5.0",
    "next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "cypress": "^7.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "standard-version": "^9.2.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
